{
  "name": "@solaudit/storage",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --out-dir dist --external @aws-sdk/client-s3 --external @aws-sdk/s3-request-presigner",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "tsup": "^8.4.0",
    "typescript": "^5.4.5"
  }
}
