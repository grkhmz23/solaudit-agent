# Required in production (min 16 chars)
API_KEY=

# Postgres (Neon/Supabase)
DATABASE_URL=postgresql://user:pass@host:5432/solaudit?sslmode=require

# Redis (Upstash)
REDIS_URL=rediss://default:xxx@xxx.upstash.io:6379

# R2 Storage
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=solaudit-artifacts

# Optional
NODE_ENV=production
